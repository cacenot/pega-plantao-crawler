# Credenciais PegaPlantão
PP_EMAIL=seu_email@exemplo.com
PP_PASSWORD=sua_senha

# Configurações opcionais
PP_HEADLESS=true
PP_TIMEOUT=30000

# ============================================
# CFM - Crawler de Médicos
# ============================================
# Navegador visível para resolver captcha manualmente
CFM_HEADLESS=true
CFM_TIMEOUT=40000

# Quantidade de médicos por página
CFM_PAGE_SIZE=900

# Delays em segundos (rate limiting)
CFM_DELAY=0.800
CFM_FOTO_DELAY=0.3

# Buscar fotos e detalhes dos médicos
CFM_FETCH_FOTOS=true

# Diretório de saída
CFM_OUTPUT_DIR=data

# PostgreSQL
CFM_DATABASE_URL=postgresql://postgres:postgres@localhost:5432/qp_crawler

# Redis
CFM_REDIS_URL=redis://localhost:6379/0
CFM_CAPTCHA_TTL=900
